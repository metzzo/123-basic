?IF DEFINED(OTTBASIC) AND NOT DEFINED(OTT_HEADER)
	?DEFINE OTT_HEADER
	TYPE TObject
		FUNCTION ToString$:
			RETURN "Object"
		ENDFUNCTION
		FUNCTION Equals%: Obj AS TObject
			IF Obj = self
				RETURN TRUE
			ELSE
				RETURN FALSE
			ENDIF
		ENDFUNCTION
		FUNCTION ToHashCode%:
			RETURN 0
		ENDFUNCTION
	ENDTYPE
?ENDIF
